{
  "tokenStore": "JSON",
  "botToken": "APP_BOT_USER_TOKEN",
  "botDiscordGame": "Minecraft",
  "prefixBlacklist": ["!"],
  "ignoreBots": false,
  "minecraftBroadcastTemplate": "&2<BROADCAST> %s",
  "channels": [
    {
      "discordId": "DISCORD_PUBLIC_CHANNEL_ID",
      "discordInviteCode": "DISCORD_INVITATION_CODE",
      "consoleCommandPrefix": "/",
  	  "consoleCommandRole": ["Owner"],
      "discord": {
        "publicChat": {
          "authenticatedChatTemplate": "%s",
          "anonymousChatTemplate": "`%a:` %s"
        },
        "joinedTemplate": "_%s just joined the server_",
        "leftTemplate": "_%s just left the server_",
        "serverUpMessage": "Server has started.",
        "serverDownMessage": "Server has stopped.",
        "broadcastTemplate": "_<BROADCAST> %s_",
        "deathTemplate": "**%s**"
      },
      "minecraft": {
        "chatTemplate": "&7<%a> &f%s",
        "attachment": {
          "template": "&3[Attachment]",
          "hoverTemplate": "Click to open attachment.",
          "allowLink": true
        },
        "mention": {
          "userTemplate": "@%s",
          "roleTemplate": "@%s",
          "everyoneTemplate": "&6@%s",
          "channelTemplate": "&9#%s"
        }
      }
    }
  ]
}